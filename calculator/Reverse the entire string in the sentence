#reverse a sentence
def rev(s):
    splt = s.split()
    str1 = splt[::-1]
    result = ''.join(str1)
    return result
#reverse all letter in sentence
def rev1(s):
    splt1 = s.split()
    str2 = []
    for i in splt1:
        str2.append(i[::-1])
    result = ' '.join(str2)
    return result
s = str(input("Enter a string: "))
print(rev(s))
print(rev1(s))




